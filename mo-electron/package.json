{
  "name": "log-electron-viewer",
  "private": true,
  "version": "0.1.0",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "electron": "cross-env NODE_ENV=development electron .",
    "electron:5173": "cross-env NODE_ENV=development VITE_DEV_SERVER_URL=http://localhost:5173 electron .",
    "electron:5174": "cross-env NODE_ENV=development VITE_DEV_SERVER_URL=http://localhost:5174 electron .",
    "electron:custom": "cross-env NODE_ENV=development electron .",
    "build": "vite build",
    "dist": "npm run build && electron-builder",
    "dist:mac": "npm run build && electron-builder --mac",
    "dist:win": "npm run build && electron-builder --win",
    "dist:linux": "npm run build && electron-builder --linux"
  },
  "build": {
    "appId": "com.asa.logviewer",
    "productName": "ASA Log Viewer",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "main.js",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.developer-tools"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.39.5",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.525.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hotkeys-hook": "^5.1.0",
    "react-json-tree": "^0.20.0",
    "react-window": "^1.8.11"
  },
  "devDependencies": {
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.7",
    "@types/react-window": "^1.8.8",
    "@vitejs/plugin-react": "^4.2.2",
    "cross-env": "^7.0.3",
    "electron": "^29.0.0",
    "electron-builder": "^26.0.12",
    "typescript": "^5.4.2",
    "vite": "^5.2.0"
  }
}
